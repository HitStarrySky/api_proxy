{
  "name": "api-proxy",
  "description": "api 代理工具",
  "version": "1.0.0",
  "author": "alqmc",
  "scripts": {
    "dev": "vite",
    "build": "gulp --require sucrase/register/ts -f build/gulpfile.ts",
    "clear": "rimraf dist",
    "update:version": "sucrase-node build/utils/version.ts",
    "push": "git push origin master && git push githup master",
    "lint:js": "eslint -c ./.eslintrc.js --fix --ext .jsx,.js,.vue \"src/**/*.{jsx,js,vue}\" ",
    "lint:style": "stylelint  \"src/**/*.vue\" --custom-syntax postcss-html",
    "lint:less": "stylelint \"src/**/*.less\" --syntax less",
    "log": "conventional-changelog -p custom-config  -i CHANGELOG.md -s -n ./changelog-option.js",
    "prepare": "husky install"
  },
  "keywords": [
    "chrome",
    "chrome-extension"
  ],
  "license": "ISC",
  "devDependencies": {
    "@alqmc/build-ts": "^0.0.9",
    "@alqmc/build-utils": "^0.0.5",
    "@alqmc/build-vue": "0.0.5",
    "@alqmc/eslint-config": "^0.0.4",
    "@commitlint/cli": "^9.1.2",
    "@commitlint/config-angular": "^8.3.4",
    "@commitlint/config-conventional": "^9.1.2",
    "@rollup/plugin-replace": "^4.0.0",
    "@types/gulp": "^4.0.9",
    "@types/gulp-autoprefixer": "^0.0.33",
    "@types/gulp-clean-css": "^4.3.0",
    "@types/gulp-less": "^0.0.32",
    "@types/lodash.clonedeep": "^4.5.7",
    "@types/node": "^17.0.25",
    "@types/rollup-plugin-css-only": "^3.1.0",
    "@vitejs/plugin-vue": "^2.2.2",
    "@vue/compiler-sfc": "^3.0.7",
    "@vue/tsconfig": "^0.1.3",
    "chrome-types": "^0.1.96",
    "commitizen": "^4.1.2",
    "compare-func": "^2.0.0",
    "conventional-changelog-cli": "^2.1.1",
    "conventional-changelog-custom-config": "^0.3.1",
    "cz-conventional-changelog": "^3.3.0",
    "fast-glob": "^3.2.11",
    "gulp": "^4.0.2",
    "gulp-autoprefixer": "^8.0.0",
    "gulp-clean-css": "^4.3.0",
    "gulp-less": "^5.0.0",
    "husky": "^8.0.1",
    "kolorist": "^1.5.1",
    "lint-staged": "^10.5.4",
    "node-html-parser": "^5.3.3",
    "postcss": "^8.4.14",
    "postcss-html": "^1.4.1",
    "postcss-less": "^6.0.0",
    "prettier": "^2.6.2",
    "rimraf": "^3.0.2",
    "rollup": "^2.68.0",
    "rollup-plugin-postcss": "^4.0.2",
    "stylelint": "^14.9.1",
    "stylelint-config-html": "^1.0.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recommended-less": "^1.0.4",
    "stylelint-config-recommended-vue": "^1.4.0",
    "stylelint-config-standard": "^26.0.0",
    "sucrase": "^3.20.3",
    "tslib": "2.0.0",
    "typescript": "^4.5.5",
    "vite": "^2.8.6"
  },
  "dependencies": {
    "element-plus": "^2.2.6",
    "lodash.clonedeep": "^4.5.0",
    "vue": "^3.2.37",
    "vue-router": "^4.0.16",
    "vue-simple-json": "^1.0.8"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}